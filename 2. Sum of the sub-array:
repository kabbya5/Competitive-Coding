<?php

	 $x = array(
	   1,	   3,	   4,	   5,	   7,	   8,
	   16,	33,44,222,11,77,99
	   );
	   
	   $N = count($x);
	   echo "$N \n";
	   $k = rand(0,$N);
	   echo $k. "\n";
	   
	   $sum_array = array_slice($x,0,$k);
	   
	   echo array_sum($sum_array);
?>

class Solution {

    /**
     * @param Integer[] $nums
     * @param Integer $k
     * @return NULL
     */

    function reverse(&$nums,$start, $end){
        while($start < $end){
            $temp = $nums[$start];
            $nums[$start] = $nums[$end];
            $nums[$end] = $temp;
            $start++;
            $end--;
        }
    }
    function rotate(&$nums, $k) {
        $n = count($nums);
        $k = $k % $n;

        $this->reverse($nums, 0, $n-1);
        $this->reverse($nums,0,$k-1);
        $this->reverse($nums,$k,$n-1);
        
        return $nums;
    }

    
}
